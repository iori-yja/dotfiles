URxvt*foreground: snow
URxvt*background: black
URxvt*saveLines:  20000
URxvt*scrollstyle: next
URxvt*scrollBar: false
URxvt*shading:  40
URxvt*cursorBlink:  True
URxvt*cursorColor:  tomato
URxvt*tintColor: gray
URxvt*inheritPixmap: True
URxvt*preeditType: OnTheSpot
URxvt*perl-ext: xim-onthespot
URxvt*termName: rxvt-unicode
URxvt*depth: 32
URxvt*imFont: -*-medium-r-*-10-*
URxvt*font:xft:Inconsolata:style=medium:size=9:antialias=true

#urxvt*font:\
#-mplus-gothic-medium-r-normal--12-*-*-*-*-60-*,\
#-mplus-gothic-medium-r-normal--12-*-*-*-*-120-
URxvt.perl-ext-common: default,matcher
URxvt.urlLauncher: firefox
URxvt.matcher.button: 2
URxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]

Xcursor.theme: Premium
XTerm*font:     -efont-fixed-medium-r-normal--14-140-75-75-c-70-iso10646-1

XLock.mode: blank
XLock.description: off
XLock.echokeys: on
XLock.echokey: *
XLock.font: -*-helvetica-medium-r-*-*-34-*-*-*-*-*-*
XLock.planfont: -misc-fixed-medium-r-normal--20-*-*-*-*-*-*
XLock.background: Black
XLock.foreground: White
XLock.username: Name:
XLock.password: Password:
XLock.info:
XLock.validate: Validating login...
XLock.invalid: Invalid login.
XLock.invalidCapsLock: Invalid login, Caps Lock on.
XLock.attempt: failed attempt.
XLock.attempts: failed attempts.

